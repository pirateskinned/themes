<script>
$(document).ready(function() {
    $('#nav-trigger').click(function() {
        $('.entries').fadeOut();
        $('.popup').fadeIn();
    });
    
    $('.pop-close span').click(function() {
        $('.popup').fadeOut();
        $('.entries').fadeIn();
    });
});
</script>

